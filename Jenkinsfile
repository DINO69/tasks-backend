pipeline{
    agent any
    stages{
        stage("Check Projeto"){
            echo "Check Projeto"
        }
        stage("Check Projeto"){
            echo "Check Projeto"
        }
    }
    post{
        always{
            echo "Check always"
        }
        sucess{
            echo "Post - Sucess"
        }
        failure{
            echo "Post - failure"
        }
    }

}